import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int M = sc.nextInt();
		int[] X = new int[M];
		int[] Y = new int[M];
		int[] ball = {1, 2, 3};
    
		for(int i = 0; i < M ; i++) {
			X[i] = sc.nextInt();
			Y[i] = sc.nextInt();
			
			int tmp = ball[Y[i]-1];
			ball[Y[i]-1] = ball[X[i]-1];
			ball[X[i]-1] = tmp;
		}
		
		System.out.println(ball[0]);
	}

}
